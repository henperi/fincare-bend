  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "seed": "babel-node server/db/seeders/index.js",
    "seed:admin": "babel-node server/db/seeders/seedSuperAdmin.js",
    "migrate": "sequelize db:migrate",
    "migrate:drop": "sequelize db:migrate:undo:all",
    "migrate:reset": "sequelize db:migrate:undo:all && npm run migrate",
    "migrate:stepBack": "sequelize db:migrate:undo",
    "start:dev": "npm run migrate && nodemon --watch server --watch .env --watch package.json --watch .eslintrc server/index.js --exec babel-node --presets es2015,stage-2",
    "restart": "npm run build && npm run migrate:reset && node build/index.js",
    "start": "npm run build && npm run migrate && node build/index.js",
    "build": "babel ./server -d build/"
  },
